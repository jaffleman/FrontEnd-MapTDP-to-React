
body {
    
    font-family: Courier New, monospace;
    background-image: url(./img/background/background.jpg);
    background-attachment: fixed;
    background-position: fixed;
    background-size: cover;
    background-blend-mode: luminosity;
    user-select: none;
  }
  .line-separator{
    height: 2px;
    border-radius: 5px;
    box-shadow: 3px 3px 10px 3px black;
    font-family: Courier New, monospace;
    border: 1px solid black;
    text-align: center;
    margin: 30px 15px 5px 15px;
    background-color: hsl(36, 90%, 48%);
    cursor: default;
  }
  .text-separator{
    border-radius: 5px;
    /* box-shadow: 3px 3px 10px 3px black; */
    font-family: Courier New, monospace;
    border: 1px solid black;
    text-align: center;
    /* width: auto; */
    padding: 4px;
    margin: -22px 40% 30px 40%;
    color: #0b1352;
    background-color: hsl(36, 90%, 48%);
    cursor: default;
  }
  .main{
    padding-top: 65px;
    padding-bottom: 65px;
  }
  .dark{
    border-radius: 5px;;
    box-shadow: 3px 3px 10px 3px black;
    font-family: Courier New, monospace;
    border: 1px solid black;
    text-align: center;
    margin: 2%;
    padding-top: 2px;
    padding-bottom: 2px;
    color: #ffffff;
    background-color: hsla(0, 0%, 0%, 0.844);
    cursor: default; 
  }
  
  .titleTextStyle{
    margin: 5px;
    text-decoration: underline;
    -webkit-text-stroke-width: thin;
  }
  .title{
    box-shadow: 3px 3px 10px 3px black;
    font-family: Courier New, monospace;
    border: 0;
    text-align: center;
    padding: 4px;
    margin: 0px;
    color: #0b1352;
    background-color: hsl(36, 94%, 47%);
    cursor: default;
    border-bottom-left-radius: 10px;
    border-bottom-right-radius: 10px;
    position: fixed;
    /*display: flex;*/
    left: 0px;
    z-index:  1;
    /*bottom: 670px;*/
    top: 0px;
    right: 0px;
    height: 40px;
  }
  
  h2{
    border-radius: 5px;
    box-shadow: 3px 3px 10px 3px black;
    font-family: Courier New, monospace;
    border: 1px solid black;
    text-align: center;
    padding: 4px;
    margin: 10px 5px 5px 5px;
    color: #0b1352;
    background-color: hsl(36, 46%, 65%);
    cursor: default;
  }
  h3{
    border-radius: 5px;
    box-shadow: 3px 3px 10px 3px black;
    font-family: Courier New, monospace;
    border: 1px solid black;
    text-align: center;
    /* width: auto; */
    padding: 4px;
    margin: 10px 5px 5px 5px;
    color: #0b1352;
    background-color: hsl(36, 46%, 65%);
    cursor: default;
  }
  
  h4{
    border-radius: 5px;
    padding: 2px;
    box-shadow: 3px 3px 10px 3px black;
    margin: 5px 15px;
    width: auto;
    border: 1px solid black;
    font-family: Courier New, monospace;
    text-align: center;
    color: #0b1352;
    background-color: hsl(54, 90%, 48%);
    cursor: default;
  } 
  h5{
    border-radius: 5px;
    box-shadow: 3px 3px 10px 3px black;
    padding: 1px;
    margin: 5px 25px;
    /* width: 265px; */
    border: 1px solid black;
    font-family: Courier New, monospace;
    text-align: center;
    color: #0b1352;
    background-color: hsl(108, 90%, 48%);
    cursor: default;
  }
  
  spam.optI{
