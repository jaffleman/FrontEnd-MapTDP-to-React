  #ButtonStyle{
    color: #0b1352;
    margin-block-start: 1em;
    margin-block-end: 1em;
    margin-inline-start: 0px;
    margin-inline-end: 0px;
    font-weight: bold;
    box-shadow: 3px 3px 10px 3px black;
    width: 99%;
    height: 40px;
    border: 1px black;
    background-color: hsl(36, 46%, 65%);
    display: block;
    margin: auto;
    border-bottom-right-radius: 5px;
    border-bottom-left-radius: 5px;
  }
  .bandoValider{
    margin-block-start: 1em;
    margin-block-end: 1em;
    margin-inline-start: 0px;
    margin-inline-end: 0px;
    box-shadow: 3px 3px 10px 3px black;
    width: -webkit-fill-available;
    height: 40px;
    border: 1px black;
    background-color: hsl(36, 46%, 65%);
    display: block;
    margin: 0 2px 0 2px;
    border-bottom-right-radius: 5px;
    border-bottom-left-radius: 5px;

  }
  .ButtonStyle{
    color: #0b1352;
    margin: 4px 0px auto 0px;
    padding: 6px 30px;
    border-radius: 5px;
    background-color: hsl(36, 46%, 65%);
    font-weight: bold;
    border-color: black;
  }
  textarea{
    margin: 0px 2px 0px 2px;
    background-color: lavender;
    display: block;
    width: -webkit-fill-available;
  }

  .bandoRechercheTdp{
    display: block;
    font-size: 1.17em;
    margin-block-start: 1em;
    margin-block-end: 1em;
    margin-inline-start: 0px;
    margin-inline-end: 0px;
    font-weight: bold;
    border-top-left-radius: 5px;
    border-top-right-radius: 5px;
    box-shadow: 3px 3px 10px 3px black;
    font-family: Courier New, monospace;
    text-align: center;
    height: 40px;
    padding: 4px;
    margin: 2px 2px 0px 2px;
    color: #0b1352;
    background-color: hsl(36, 46%, 65%);
    cursor: default;
    margin-top:'10px';
    font-weight:'bold';
    text-transform:'uppercase';
  }
